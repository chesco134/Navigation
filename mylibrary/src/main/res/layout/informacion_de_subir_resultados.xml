<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:orientation="vertical" android:layout_width="match_parent"
    android:layout_height="match_parent">

    <TextView
        android:id="@+id/informacion_de_subir_resultados_texto"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_marginTop="20sp"
        android:layout_marginStart="12sp"
        android:layout_marginLeft="12sp"
        android:layout_marginRight="12sp"
        android:layout_marginEnd="12sp"
        android:textSize="18sp"/>
    <TextView
        android:id="@+id/informacion_de_subir_resultados_vinculo"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_below="@+id/informacion_de_subir_resultados_texto"
        android:layout_marginTop="12sp"
        android:layout_alignStart="@+id/informacion_de_subir_resultados_texto"
        android:layout_alignLeft="@+id/informacion_de_subir_resultados_texto"
        android:layout_alignEnd="@+id/informacion_de_subir_resultados_texto"
        android:layout_alignRight="@+id/informacion_de_subir_resultados_texto"
        android:autoLink="web"/>

</RelativeLayout>